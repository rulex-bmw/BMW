<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rulex.dsm.bean.TestDao">

    <update id="editTest" parameterType="com.rulex.dsm.pojo.Test">
        UPDATE bmw_test set wallet=#{wallet} where age <![CDATA[ <= ]]> #{age};
    </update>

    <insert id="insertTest" parameterType="com.rulex.dsm.pojo.Test">
        insert into bmw_test(username,age,phone,tall,wallet) values (#{username},#{age},#{phone},#{tall},#{wallet});
    </insert>
</mapper>
