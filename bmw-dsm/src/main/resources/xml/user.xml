<!--拦截规则和数据库信息-->
<schema version="1.0">

    <record id="1" name="User" groupable="true" table="bmw_user" pojo="com.rulex.dsm.pojo">
        <!--只是规定primaryKey，不上链-->
        <primary>
            <key name="id" column="id" type="Integer"/>
            <!--<key name="" column="" type=""/>-->
            <!--<key name="" column="" type=""/>-->
        </primary>
        <!--上链信息-->
        <field name="id" column="id" type="Integer" isnull="false" transforable="false" length="4" fieldId="1"
               isprimaykey="true"></field>
        <field name="name" column="username" type="String" isnull="false" transforable="false"
               length="20" fieldId="2"></field>
        <field name="age" column="age" type="Integer" isnull="false" transforable="false" length="50"
               fieldId="3"></field>
    </record>
</schema>